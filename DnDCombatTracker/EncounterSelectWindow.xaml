<Window x:Class="DnDCombatTracker.EncounterSelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DnDCombatTracker"
        mc:Ignorable="d"
        Title="EncounterSelectWindow" Height="450" Width="800" Background="#FF9B9B9B">
    <Grid>
        <ListBox Name="encounterListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="242,54,242,22" MouseDoubleClick="encounterListBox_MouseDoubleClick"/>
        <Button Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="621,24,0,0" VerticalAlignment="Top" Width="150" Height="50" Click="cancelButton_Click"/>

    </Grid>
</Window>
